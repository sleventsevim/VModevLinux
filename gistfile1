(function() {
var bkm_url=prompt("URL:", "http://");
var bkm_filename=prompt("File:", "");
if (bkm_url!=null && bkm_filename != null) {
    if (typeof(bkm_nw)=="undefined"||bkm_nw.closed){
        var bkm_nw=window.open(bkm_url, bkm_filename+".pdf", "width=200,height=50,filename="+bkm_filename+".pdf");
    }
}
})();